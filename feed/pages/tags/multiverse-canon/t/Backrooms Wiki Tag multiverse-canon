&lt;div class=&quot;class-name&quot;&gt;
&lt;p&gt;&lt;span class=&quot;custom {$title-class}&quot;&gt;{$title-class}&lt;/span&gt;&lt;span class=&quot;title-default {$title-class}&quot;&gt;Class:&lt;/span&gt;&lt;span style=&quot;white-space: pre-wrap;&quot;&gt;&amp;#32;&lt;/span&gt;&lt;/p&gt;
&lt;div class=&quot;class-title class-0M&quot;&gt;
&lt;p&gt;&lt;span class=&quot;class-Metaphysical&quot;&gt;Metaphysical&lt;/span&gt;&lt;span class=&quot;pts-default&quot;&gt;&lt;span style=&quot;white-space: pre-wrap;&quot;&gt;&amp;#32;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;
&lt;/div&gt;
&lt;div class=&quot;sub-attribute&quot;&gt;
&lt;p&gt;&lt;span class=&quot;custom {$title-sub}&quot;&gt;{$title-sub}&lt;/span&gt;&lt;span class=&quot;title-default {$title-sub}&quot;&gt;Area of Effect:&lt;/span&gt;&lt;span style=&quot;white-space: pre-wrap;&quot;&gt;&amp;#32;&lt;/span&gt;The Backrooms&lt;/p&gt;
&lt;/div&gt;
&lt;div class=&quot;sub-classes&quot;&gt;
&lt;div class=&quot;sub-class empty&quot; style=&quot;background-color: transparent; width: 15%; border: 0;&quot;&gt;&lt;/div&gt;
