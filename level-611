        document.getElementById("consent-box").style.display = window["__tcfapi"] ? "" : "none";
        } else {
        document.addEventListener(
                "nitroAds.loaded",
                () =>
                (document.getElementById("consent-box").style.display = window["__tcfapi"] ? "" : "none")
                );
    }
    </script>
